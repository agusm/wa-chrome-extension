{
  "name": "WA Bot",
  "version": "2.5",
  "description": "Auto follow up customer",
  "permissions": ["contentSettings", "tabs", "activeTab", "http://bot.xxx.com/", "*://*.whatsapp.com/*"],
  "content_scripts": [
    {
      "js": ["jquery-3.3.1.min.js", "inject.js"],
      "matches": ["*://*/*"]
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "contentSettings.png",
    "48": "contentSettings.png",
    "128": "contentSettings.png"
  }
}
